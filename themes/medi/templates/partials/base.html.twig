<!doctype html>
<html lang="{{ grav.language.getLanguage ?: (theme_config.default_lang|default('en')) }}">

<head>
{% block head %}
{% block head_meta %}
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />

        <!-- Generic -->
{% for meta in page.metadata %}
        <meta {% if meta.name %}name="{{ meta.name }}" {% endif %}{% if meta.http_equiv %}http-equiv="{{ meta.http_equiv }}" {% endif %}{% if meta.charset %}charset="{{ meta.charset }}" {% endif %}{% if meta.property %}property="{{ meta.property }}" {% endif %}{% if meta.content %}content="{{ meta.content }}" {% endif %} />
{% endfor %}
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <meta name="description" content="{{ site.metadata.description }}" />

        <!-- Facebook Open Graph -->
        <meta property="og:url" content="{{ page.url(true, true) }}" />
        <meta property="og:type" content="article" />
        <meta property="og:title" content="{% if page.header.title and (page.url != base_url or 'error' == page.template) %}{{ page.header.title }}|{% endif %}{{ site.title }}" />
        <meta property="og:description" content="{{ site.metadata.description }}" />
        <meta property="og:image" content="http://static01.nyt.com/images/2015/02/19/arts/international/19iht-btnumbers19A/19iht-btnumbers19A-facebookJumbo-v2.jpg" />
{% endblock %}

{% block head_title %}
        <title>{% if page.header.title and (page.url != base_url or 'error' == page.template) %}{{ page.header.title }}|{% endif %}{{ site.title }}</title>
{% endblock %}

        <!-- <link rel="manifest" href="site.webmanifest"> -->
{% set favicon_url = 'theme://assets/img/favicon.ico' %}
{% if theme_config.favicon_url %}{% set favicon_url = theme_config.favicon_url %}{% endif %}
        <link rel="canonical" href="{{ page.url(true, true) }}" />
        <link rel="shortcut icon" href="{{ url(favicon_url) }}" alt="{{ site.title|e('html_attr') }}" />
{% endblock %}

    <!-- CSS here -->
{% block head_css %}
    {% if theme_config.cdn %}
    {% do assets.add('https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css', 101) %}
    {% else %}
    {% do assets.add('theme://assets/css/font-awesome.min.css', 101) %}
    {% endif %}

    {% do assets.add('theme://assets/css/bootstrap.min.css', 102) %}
    {% do assets.add('theme://assets/css/owl.carousel.min', 102) %}
    {% do assets.add('theme://assets/css/magnific-popup.css', 102) %}
    {% do assets.add('theme://assets/css/font-awesome.min.css', 100) %}
    {% do assets.add('theme://assets/css/themify-icons.css', 100) %}
    {% do assets.add('theme://assets/css/nice-select.css', 100) %}
    {% do assets.add('theme://assets/css/flaticon.css', 100) %}
    {% do assets.add('theme://assets/css/gijgo.css', 100) %}
    {% do assets.add('theme://assets/css/animate.css', 100) %}
    {% do assets.add('theme://assets/css/slicknav.css', 100) %}
    {% do assets.add('theme://assets/css/style.css', 100) %}
    {% do assets.add('theme://assets/css/responsive.css', 100) %}
{{ assets.css()|raw }}

{% block head_css_extra %}{% endblock %}
{% block head_js %}
{# HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries #}
{# WARNING: Respond.js doesn't work if you view the page via file:// #}
{% if browser.getBrowser == 'msie' and browser.getVersion >= 8 and browser.getVersion <= 9 %}
{% do assets.add('https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js') %}
{% do assets.add('https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js') %}
{% endif %}
{{ assets.js()|raw }}
{% endblock %}
{% endblock %}
</head>
<body>

<!--[if lte IE 9]>
<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
<![endif]-->

{% include 'partials/header.html.twig' %}

{% block body %}{% endblock %}

    <!-- JS here -->
    {% block body_js %}
        {% if theme_config.cdn %}
            {% do assets.addJs('//code.jquery.com/jquery-1.12.4.min.js', 104) %}
        {% else %}
            {% do assets.addJs('theme://assets/js/vendor/jquery-1.12.4.min.js', 104) %}
        {% endif %}
        
        {% do assets.addJs('plugin://simplesearch/js/simplesearch.js', 104) %}
        {% do assets.addJs('theme://assets/js/vendor/modernizr-3.5.0.min.js', 104) %}
        {% do assets.addJs('theme://assets/js/owl.carousel.min.js', 104) %}
        {% do assets.addJs('theme://assets/js/bootstrap.min.js', 103) %}
        {% do assets.addJs('theme://assets/js/popper.min.js', 103) %}
        {% do assets.addJs('theme://assets/js/isotope.pkgd.min.js', 103) %}
        {% do assets.addJs('theme://assets/js/ajax-form.js', 103) %}
        {% do assets.addJs('theme://assets/js/waypoints.min.js', 103) %}
        {% do assets.addJs('theme://assets/js/jquery.counterup.min.js', 102) %}
        {% do assets.addJs('theme://assets/js/imagesloaded.pkgd.min.js', 103) %}
        {% do assets.addJs('theme://assets/js/scrollIt.js', 103) %}
        {% do assets.addJs('theme://assets/js/jquery.scrollUp.min.js', 102) %}
        {% do assets.addJs('theme://assets/js/wow.min.js', 103) %}
        {% do assets.addJs('theme://assets/js/nice-select.min.js', 103) %}
        {% do assets.addJs('theme://assets/js/jquery.slicknav.min.js', 102) %}
        {% do assets.addJs('theme://assets/js/jquery.magnific-popup.min.js', 102) %}
        {% do assets.addJs('theme://assets/js/plugins.js', 101) %}
        {% do assets.addJs('theme://assets/js/gijgo.min.js', 101) %}

        {% do assets.addJs('theme://assets/js/contact.js', 100) %}
        {% do assets.addJs('theme://assets/js/jquery.ajaxchimp.min.js', 100) %}
        {% do assets.addJs('theme://assets/js/jquery.form.js', 100) %}
        {% do assets.addJs('theme://assets/js/jquery.validate.min.js', 100) %}
        {% do assets.addJs('theme://assets/js/mail-script.js', 100) %}
        {% do assets.addJs('theme://assets/js/main.js', 100) %}
    {{ assets.js()|raw }}

        <script>
            jQuery('.datepicker').datepicker({
                iconsLibrary: 'fontawesome',
                icons: {
                    rightIcon: '<span class="fa fa-calendar"></span>'
                }
            });

            jQuery('.timepicker').timepicker({
                iconsLibrary: 'fontawesome',
                icons: {
                    rightIcon: '<span class="fa fa-clock-o"></span>'
                }
            });
            jQuery(document).ready(function () {
                jQuery('.js-example-basic-multiple').select();
                //jQuery('.js-example-basic-multiple').select2();
            });
        </script>
    {% endblock %}
    
</body></html>