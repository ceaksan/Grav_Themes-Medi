<div class="welcome_clicnic_area {{ class }}">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-6 col-lg-6">
                    <div class="welcome_thumb">
                        <div class="thumb_1">
                            {% set image = page.media.images|first %}
                            {{ image.html(page.title, page.title)|raw }}
                        </div>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-6">
                    <div class="welcome_docmed_info">
                        {{ content|raw }}
                        {% for welcome in page.header.welcome %}
                            <a href="{{ welcome.buttonurl }}" title="{{ welcome.buttontext }}" class="boxed-btn6">{{ welcome.buttontext }}</a>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </div>